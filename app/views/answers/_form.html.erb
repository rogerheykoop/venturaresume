<%= simple_form_for(@answer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :question %>
    <%= f.association :resume %>
    <%= f.association :user %>
    <%= f.input :include_in_print %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
