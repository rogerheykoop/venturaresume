<%= simple_form_for(@experience) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :role %>
    <%= f.association :company %>
    <%= f.association :user %>
    <%= f.input :startdate %>
    <%= f.input :enddate %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
